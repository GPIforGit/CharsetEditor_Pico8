function str2mem(data)
  local t,a,i,c,d,str,m = split(data)
  for a = 1,#t,2 do  
    m,str,i=t[a],t[a+1],1
    while i <= #str do
      c,d= ord(str,i,2)
      i+=c==255 and 2 or 1
      if (c>16) poke(m, c==255 and d or c^^0x80) m+=1
    end
  end
end

